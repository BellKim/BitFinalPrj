<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.palette.s3.FileDao">

	<insert id="upload" parameterType="com.palette.s3.FileDto" useGeneratedKeys="true" keyProperty="id">
    	INSERT INTO FILE(TITLE, FILE_PATH) 
    	VALUES(#{title}, #{filePath})
	</insert>

	<select id="getImageList" resultType="com.palette.s3.FileDto">
		SELECT TITLE, FILE_PATH 
		FROM FILE
	</select>

</mapper>