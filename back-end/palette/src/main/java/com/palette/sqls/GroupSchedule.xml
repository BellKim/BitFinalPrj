<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.palette.dao.GroupDao">
	<select id="getGroupSchedule" parameterType="com.palette.model.GroupDto" resultType="com.palette.model.CalendarDto">
		select * from FINAL.CALENDAR
		where GROUP_INFO_SEQ = #{groupInfoSeq} and AUTH=1;
	</select>
</mapper>