<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.palette.dao.GroupDao">

  <select id="getAllGroup" resultType="com.palette.model.GroupDto">
    SELECT * FROM GROUP_INFO
  </select>
  
  <select id="getOneGroup" parameterType="java.lang.Integer" resultType="com.palette.model.GroupDto">
    SELECT * FROM GROUP_INFO
    WHERE GROUP_INFO_SEQ = #{seq}
  </select>

 

</mapper>